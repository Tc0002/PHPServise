# ローカル環境用設定
APP_ENV=local
APP_DEBUG=true
APP_URL=http://localhost

# データベース設定（ローカル）
DB_CONNECTION=pgsql
DB_HOST=localhost
DB_PORT=5432
DB_DATABASE=laravel
DB_USERNAME=laravel
DB_PASSWORD=secret

# Redis設定（ローカル）
REDIS_HOST=localhost
REDIS_PORT=6379

# MinIO設定（ローカル）
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
AWS_ENDPOINT=http://localhost:9000
S3_BUCKET=laravel-product-system

# Elasticsearch設定（ローカル）
ELASTICSEARCH_HOST=localhost
ELASTICSEARCH_PORT=9200
